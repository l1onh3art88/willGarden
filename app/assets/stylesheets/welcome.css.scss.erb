.container{
height: 100em;  
margin: 0px;
padding: 0px;
width: 100%;
position: fixed;
top: 0px;
left: 0px;
}

#hamburger{
  position: relative;
  top:2em;
  left: 3em;
  z-index: 1000;
}
.fill{
  width:100%;
  height:100%;
  background-position: center;
}
#landingpage{
  background-size: 100%;
  height: 100%;
}
.navmenu{
  display: none;
  background-color: white;
  height: 100%;
  width:15%;
  margin-top: 4em;
}
.navmenu li{
  list-style-type: none;
  height: 5%;
}
.navmenu li a{
  color: black;
  position: relative;
  left: 4em;
  top: 2em;
}
.navmenu li:hover{
  background:grey;
}
.carousel-inner{
  height: 100%;
}
a.left.carousel-control, a.right.carousel-control{
  height:0px;
}